{
  "name": "tigris-movie-db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tigris:seed": "npm run tigris:setup && curl https://raw.githubusercontent.com/prust/wikipedia-movie-data/master/movies.json --output ./scripts/movies.json && cat ./scripts/movies.json | tigris import --append -p tigris-movie-db movies",
    "tigris:setup": "npx ts-node ./scripts/setup.ts",
    "predev": "npm run tigris:setup",
    "dev": "next dev",
    "prebuild": "npm run tigris:setup",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@tigrisdata/core": "^1.1.0",
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "next": "13.4.2",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
